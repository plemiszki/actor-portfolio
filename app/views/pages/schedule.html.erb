<div class="container">
  <div class="row schedule">
    <% @events.each do |event| %>
      <div class="col-xs-12 col-md-offset-1 col-md-10">
        <p><%= event.time.strftime("%B %-d - %-l:%M %P") %></p>
        <p><%= event.title %></p>
        <%= markdown(event.text) %>
      </div>
    <% end %>
  </div>
</div>
